{
  "manifest_version": 2,
  "name": "Famou5 Women's Day 2017",
  "description": "Badass women of Canada.",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab"
  ]
}
